SRC_DIR_F90d1 = ../src/src_common/

SRC_DIR_f90d2 = ../src/src_fcc/

SRC_DIR_f90d1 = ../src/src_common/
OBJS_DIR = ../obj/eddypro_fcc/
EXE_DIR = ../bin/

EXE = eddypro_fcc
FC = gfortran
CFLAGS = -pedantic -Wall -Wextra -Wcharacter-truncation -Wunderflow -Wno-compare-reals -fall-intrinsics -fbounds-check -std=f2003 -O3  -J$(OBJS_DIR) 
LIBS = 

VPATH = $(SRC_DIR_F90d1):$(OBJS_DIR):$(SRC_DIR_f90d2):$(OBJS_DIR):$(SRC_DIR_f90d1):$(OBJS_DIR)
OBJS = $(addprefix $(OBJS_DIR), $(OBJS_F90d1) $(OBJS_f90d2) $(OBJS_f90d1))

all : $(EXE)

$(EXE) : $(OBJS_F90d1) $(OBJS_f90d2) $(OBJS_f90d1)
	@mkdir -p $(EXE_DIR)
	$(FC) -o $(EXE_DIR)$(EXE) $(OBJS)  

$(OBJS_F90d1):
	@mkdir -p $(OBJS_DIR)
	$(FC) $(CFLAGS) -c $(SRC_DIR_F90d1)$(@:.o=.F90) -o $(OBJS_DIR)$@

$(OBJS_f90d2):
	@mkdir -p $(OBJS_DIR)
	$(FC) $(CFLAGS) -c $(SRC_DIR_f90d2)$(@:.o=.f90) -o $(OBJS_DIR)$@

$(OBJS_f90d1):
	@mkdir -p $(OBJS_DIR)
	$(FC) $(CFLAGS) -c $(SRC_DIR_f90d1)$(@:.o=.f90) -o $(OBJS_DIR)$@

clean :
	rm -f $(OBJS_DIR)*.*
	rm -f $(EXE_DIR)$(EXE)

